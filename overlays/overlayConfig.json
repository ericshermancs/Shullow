{
  "sites": {
    "zillow.com": {
      "overlay": "ZillowOverlay",
      "mapType": "mapbox",
      "priority": 100,
      "selectors": [".mapboxgl-map"],
      "features": {
        "shadowDOM": false,
        "redux": false,
        "networkSniffing": false
      }
    },
    "redfin.com": {
      "overlay": "RedfinOverlay",
      "mapType": "google",
      "priority": 100,
      "selectors": [".gm-style", "[data-rf-test-id=\"map\"]"],
      "features": {
        "shadowDOM": false,
        "redux": true,
        "networkSniffing": false,
        "globalVariables": ["RF_CONTEXT", "__map_bounds__"]
      }
    },
    "homes.com": {
      "overlay": "HomesComOverlay",
      "mapType": "google",
      "priority": 100,
      "selectors": ["gmp-map", ".gm-style"],
      "features": {
        "shadowDOM": true,
        "redux": false,
        "networkSniffing": false
      }
    },
    "onekeymls.com": {
      "overlay": "OneKeyOverlay",
      "mapType": "mapbox",
      "priority": 100,
      "selectors": [".mapboxgl-map"],
      "features": {
        "shadowDOM": false,
        "redux": false,
        "networkSniffing": false
      }
    },
    "realtor.com": {
      "overlay": "RealtorOverlay",
      "mapType": "auto",
      "priority": 100,
      "selectors": [".mapboxgl-map", ".gm-style", "gmp-map"],
      "features": {
        "shadowDOM": true,
        "redux": false,
        "networkSniffing": false
      }
    }
  },
  "defaults": {
    "overlay": "GenericMapOverlay",
    "mapType": "auto",
    "priority": 1,
    "features": {
      "shadowDOM": true,
      "redux": false,
      "networkSniffing": false
    },
    "genericNativeMarkerPatterns": [
      {
        "name": "mapbox-site-markers",
        "selectors": [".mapboxgl-popup", "[class*='marker'][class*='mapbox']"],
        "mapType": "mapbox",
        "confidence": "medium"
      },
      {
        "name": "google-standard-markers",
        "selectors": ["[role='button'][aria-label*='marker']", ".gm-ui-hover-effect"],
        "mapType": "google",
        "confidence": "medium"
      },
      {
        "name": "leaflet-markers",
        "selectors": [".leaflet-marker", ".leaflet-popup-content"],
        "mapType": "leaflet",
        "confidence": "high"
      },
      {
        "name": "common-pin-patterns",
        "selectors": ["[class*='pin']", "[data-marker-id]", "[data-place-id]", "[class*='place']"],
        "mapType": "any",
        "confidence": "low"
      },
      {
        "name": "aria-labeled-markers",
        "selectors": ["[aria-label*='pin']", "[aria-label*='location']", "[aria-label*='place']"],
        "mapType": "any",
        "confidence": "low"
      }
    ]
  },
  "domainAliases": {
    "www.zillow.com": "zillow.com",
    "www.redfin.com": "redfin.com",
    "www.homes.com": "homes.com",
    "www.onekeymls.com": "onekeymls.com",
    "www.realtor.com": "realtor.com"
  }
}
